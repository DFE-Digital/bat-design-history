---
permalink: /search.json
---
[{% for item in collections["search-index"] %}
  {
    "data": {
      "title": {{ item.data.title | dump | safe }},
      {% if item.data.description %}"description": {{ item.data.description | dump | safe }},{% endif %}
      "section": {{ item.data.breadcrumbs[0].text | dump | safe }},
      "layout": {{ item.data.layout | dump | safe }}
    },
    "date": {{ item.date | date | dump | safe }},
    "dateString": {{ item.date  | date("d LLLL y") | dump | safe }},
    "templateContent": {{ item.templateContent | tokenize | dump | safe }},
    "url": {{ item.url | pretty | dump | safe }}
  }{% if not loop.last %},{% endif %}
{% endfor %}]
